{
    "name": "readwise-mcp",
    "version": "1.0.0",
    "description": "MCP Server for Readwise using TypeScript",
    "scripts": {
        "build": "tsc",
        "start": "node --no-warnings dist/index.js",
        "dev": "ts-node src/index.ts",
        "simple": "ts-node src/simple-server.ts",
        "lint": "eslint src --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:tags": "jest tags",
        "test:search": "jest advanced-search",
        "test:bulk": "jest bulk-operations",
        "test:progress": "jest reading-progress",
        "test:delete": "jest delete-confirmation",
        "run-inspector": "ts-node run-inspector.ts",
        "test-mcp": "ts-node test-mcp.ts",
        "test-mcp-client": "ts-node test-mcp-client.ts",
        "mcp-test": "ts-node mcp-test.ts"
    },
    "keywords": [
        "mcp",
        "readwise",
        "claude",
        "smithery",
        "typescript"
    ],
    "author": "IAmAlexander",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.4.0",
        "axios": "^1.6.2",
        "cors": "^2.8.5",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.10.0",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2"
    },
    "engines": {
        "node": ">=16.0.0 <20.0.0"
    }
}
